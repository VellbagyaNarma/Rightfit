<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>LoginRegisterController</servlet-name>
  	<display-name>LoginRegisterController</display-name>
  	<description></description>
  	<servlet-class>com.lakshmivel.om.LoginRegisterController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Adminservlet</servlet-name>
  	<display-name>Adminservlet</display-name>
  	<description></description>
  	<servlet-class>com.admin.Adminservlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutAdmin</servlet-name>
  	<display-name>LogoutAdmin</display-name>
  	<description></description>
  	<servlet-class>com.admin.LogoutAdmin</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginRegisterController</servlet-name>
  	<url-pattern>/LoginRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Adminservlet</servlet-name>
  	<url-pattern>/Admin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutAdmin</servlet-name>
  	<url-pattern>/Logout</url-pattern>
  </servlet-mapping>
</web-app>
